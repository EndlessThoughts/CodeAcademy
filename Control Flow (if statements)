def greater_than(x,y):
  if x > y:
    return x
  if x < y:
    return y
  if x == y:
    return "These numbers are the same"
    
#Call and print greater_than()
print(greater_than(-3,-3))
print(greater_than(-10,-9))
print(greater_than(-6,6))

def graduation_reqs(credits):
  if credits < 120:
    return "Unfortunately, you cannot graduate yet."
  if credits >= 120:
    return "You have enough credits to graduate!"
    
#Call and print graduation_reqs()
print(graduation_reqs(130))
print(graduation_reqs(119))
print(graduation_reqs(120))
