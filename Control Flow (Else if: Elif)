def grade_converter(gpa):
  if gpa >= 4.0:
    return "A"
  elif gpa >= 3.0:
    return "B"
  elif gpa >= 2.0:
    return "C"
  elif gpa >= 1.0:
    return "D"
  else:
    return "F"

#Call and print gpa_converter()
print(grade_converter(4.5))
print(grade_converter(3.5))
print(grade_converter(2.5))
print(grade_converter(1.5))
print(grade_converter(0.99))
